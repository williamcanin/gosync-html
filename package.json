{
  "name": "gosync-html",
  "version": "0.1.0",
  "description": "Go!Sync HTML - Simple boilerplate with Gulp and Browser Sync for projects HTML.",
  "main": "gulpfile.js",
  "scripts": {
    "postinstall": "$(npm bin)/gulp build",
    "server": "$(npm bin)/gulp serve",
    "build": "$(npm bin)/gulp build",
    "clean": "$(npm bin)/gulp clean"
  },
  "keywords": [
    "skeleton",
    "boilerplate",
    "template",
    "html"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/williamcanin/gosync-html"
  },
  "ignore": [
    "node_modules"
  ],
  "author": "William C. Canin",
  "license": "https://opensource.org/licenses/MIT",
  "devDependencies": {
    "browser-sync": "^2.27.5",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-csso": "^4.0.1",
    "gulp-htmlmin": "^5.0.1",
    "gulp-if": "^3.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.0.0",
    "gulp-uglify": "^3.0.2",
    "npm-check-updates": "^11.8.5",
    "sass": "^1.42.1"
  },
  "dependencies": {
    "bootstrap": "^5.1.3",
    "jquery": "^3.6.0"
  }
}
